{
  "manifest_version": 3,
  "name": "Tweet4Tweet Configuration",
  "version": "0.1.1",
  "description": "Read X cookies to download configuration file for Tweet4Tweet",
  "permissions": ["cookies", "webRequest", "activeTab", "storage"],
  "host_permissions": ["https://x.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  }
}
